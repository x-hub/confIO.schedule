{   
   "index": "/index.html",
   "assetGroups": [{     
     "name": "app",
     "installMode": "prefetch",
     "resources": {
       "files": [
         "/favicon.ico",
         "/index.html"
       ],
       "versionedFiles": [
         "/*.bundle.css",
         "/*.bundle.js",
         "/*.chunk.js"
       ]
     }
   },
   {
     "name": "assets",
     "installMode": "lazy",
     "updateMode": "prefetch",
     "resources": {
       "files": [
         "/assets/**"
       ],
       "urls": [
        "https://fonts.googleapis.com/**",
        "https://cdn.jsdelivr.net/lodash/4.17.4/lodash.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css",
        "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css",
        "https://cdnjs.cloudflare.com/ajax/libs/css-loader/2.2.0/css-loader.css",
        "https://code.jquery.com/jquery-3.2.1.min.js"
       ]
     }
  }],
  "dataGroups": [
  {
    "name": "rooms-speakers-api",
    "urls": [ 
        "https://cfp.devoxx.ma/api/conferences/DevoxxMA2017/**",
        "http://cfp.devoxx.ma/api/conferences/DevoxxMA2017/**",
        "https://scontent.xx.fbcdn.net/**",
        "http://www.gravatar.com/avatar/**"
        ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 300, 
      "maxAge": "1h",
      "timeout": "10s"
    }
  }, {
    "name": "api-performance",
    "urls": [
      "http://cfp.devoxx.ma/api/conferences/DevoxxMA2017/**",
      "https://scontent.xx.fbcdn.net/**",
      "http://www.gravatar.com/avatar/**"
    ],
    "cacheConfig": {
      "maxSize": 300,
      "maxAge": "1h",
      "strategy": "performance"
    }
  }]
}